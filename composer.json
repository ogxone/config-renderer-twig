{
  "name": "ogxone/config-renderer-twig",
  "description": "Renders folders recursively and uses twig to render all .twig files",
  "keywords": ["twig", "folder", "render"],
  "license": "MIT",
  "authors": [
    {
      "name": "ogxone"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "ogxone/utils": "dev-master",
    "twig/twig": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Ogxone\\ConfigRendererTwig\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ogxone\\ConfigRendererTwigTest\\": "test/"
    }
  }
}
  